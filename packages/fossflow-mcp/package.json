{
  "name": "fossflow-mcp",
  "version": "1.0.0",
  "description": "MCP server for FossFLOW - Create and manipulate isometric diagrams via natural language",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "fossflow-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "setup:claude": "node scripts/setup-claude.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "immer": "^10.0.0",
    "uuid": "^9.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.0",
    "@types/uuid": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "fossflow",
    "isoflow",
    "diagrams",
    "isometric"
  ],
  "author": "FossFLOW Team",
  "license": "MIT"
}
